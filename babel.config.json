{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "version-inline"
  ],
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      { "useBuiltIns": "usage", "corejs": { "version": 3, "proposals": true } }
    ],
    ["@babel/preset-typescript", { "allowDeclareFields": true }]
  ]
}
