{
  "name": "rdeck",
  "version": "0.2.2",
  "private": true,
  "sideEffects": "**.css",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "npm explore @danmarshall/deckgl-typings -- npm run postinstall",
    "build": "webpack --mode production",
    "start": "webpack --watch --mode development",
    "gen:interface": "node ./scripts/generate.js",
    "generate": "npm run gen:interface && npm run styler",
    "styler": "R -e \"styler::style_dir('./R')\""
  },
  "dependencies": {
    "@danmarshall/deckgl-typings": "^3.4.7",
    "@types/d3-scale": "^2.2.0",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-map-gl": "^5.2.0",
    "core-js": "^3.6.4",
    "d3-scale": "^3.2.1",
    "deck.gl": "^8.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-map-gl": "^5.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-version-inline": "^1.0.0",
    "browserslist-config-google": "^2.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "marked": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "param-case": "^3.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.1",
    "snake-case": "^3.0.3",
    "style-loader": "^1.1.3",
    "ts-dedent": "^1.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "extends browserslist-config-google/modern"
  ],
  "prettier": {
    "printWidth": 100
  }
}
